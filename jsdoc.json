{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./lib",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/minami",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
